{
  "name": "dmarc-reporter",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "shx rm -rf dist",
    "create-env": "ts-node --project tsconfig.local.json scripts/create-env.ts",
    "infra:deploy": "npm run infra:generate && terraform -chdir=infrastructure apply --auto-approve",
    "infra:generate": "ts-node --project tsconfig.local.json -r dotenv/config infrastructure",
    "cli": "ts-node --project tsconfig.local.json -r dotenv/config src/cli"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.4.1",
    "esbuild": "^0.20.0",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "terraform-generator": "^6.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
